{
  "strict": {
    "text-font-family": "Nunito-Regular",

    "text-heading-1-font-size": 36,
    "text-heading-1-font-family": "Nunito-Bold",

    "text-heading-2-font-family": "Nunito-Bold",

    "text-heading-3-font-family": "Nunito-Bold",

    "text-heading-4-font-family": "Nunito-Bold",

    "text-heading-5-font-family": "Nunito-Bold",

    "text-heading-6-font-family": "Nunito-Bold",

    "text-subtitle-1-font-family": "Nunito-Regular",

    "text-subtitle-2-font-family": "Nunito-Regular",

    "text-paragraph-1-font-family": "Nunito-Regular",

    "text-paragraph-2-font-family": "Nunito-Regular",

    "text-caption-1-font-family": "Nunito-Regular",

    "text-caption-2-font-family": "Nunito-Regular",

    "text-label-font-family": "Nunito-Regular"

  },
  "components": {
    "Tab": {
        "meta": {
            "scope": "all",
            "parameters": {
                "textFontFamily": {
                    "type": "string"
                },
                "textFontSize": {
                    "type": "number"
                },
                "textFontWeight": {
                    "type": "number"
                },
                "textMarginVertical": {
                    "type": "number"
                },
                "textColor": {
                    "type": "string"
                },
                "iconWidth": {
                    "type": "number"
                },
                "iconHeight": {
                    "type": "number"
                },
                "iconMarginVertical": {
                    "type": "number"
                },
                "iconTintColor": {
                    "type": "string"
                }
            },
            "appearances": {
                "default": {
                    "default": true
                }
            },
            "variantGroups": {},
            "states": {
                "selected": {
                    "default": false,
                    "priority": 0,
                    "scope": "all"
                },
                "hover": {
                    "default": false,
                    "priority": 1,
                    "scope": "all"
                },
                "focused": {
                    "default": false,
                    "priority": 2,
                    "scope": "all"
                }
            }
        },
        "appearances": {
            "default": {
                "mapping": {
                    "textMarginVertical": 2,
                    "textFontSize": "text-caption-2-font-size",
                    "textFontWeight": "text-caption-2-font-weight",
                    "textFontFamily": "text-caption-2-font-family",
                    "textColor": "text-hint-color",
                    "iconWidth": 24,
                    "iconHeight": 24,
                    "iconMarginVertical": 2,
                    "iconTintColor": "text-hint-color",
                    "state": {
                        "hover": {
                            "textColor": "text-primary-hover-color",
                            "iconTintColor": "text-primary-hover-color"
                        },
                        "selected": {
                            "textColor": "text-primary-color",
                            "iconTintColor": "text-primary-color"
                        }
                    }
                }
            }
        }
    }
  }
}
